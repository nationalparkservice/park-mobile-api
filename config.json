{
  "appName": "NPMap Places-Mobile API",
  "cacheResetUrl": "http://ncrcms.nps.doi.net/purge/akam_build_eccu.jsp?path=%2Fnpmap%2Fprojects%2Fplaces-mobile%2F{{unitCode}}%2F{{path}}&email=&Submit=Submit",
  "database": {
    "cartodb": {
      "account": "nps",
      "apiKey": "./secrets/cartodb",
      "type": "cartodb"
    },
    "defaults": {
      "rootDirectory": "{{env.PWD}}/test/sql",
      "delimiter": ";",
      "fileDesignator": "file:///",
      "fileOptions": {
        "encoding": "utf8"
      }
    }
  },
  "debug": true,
  "fileLocation": "/mnt/npmap/projects/places-mobile",
  "htmlDirectory": "html",
  "mapbox": {
    "token": "./secrets/mapbox"
  },
  "tempFileLocation": "/home/npmap/places_mobile_tmp",
  "zipFields": {
    "640": {
      "commonImage": "image_640",
      "video": "relative_url",
